{
  "manifest_version": 2,
  "name": "Simple Web Scraper",
  "description": "A simple Chrome extension to scrape web data",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}